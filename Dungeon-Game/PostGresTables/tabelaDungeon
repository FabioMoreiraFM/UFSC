DROP TABLE HISTORIADUNGEON
DROP TABLE PERSONAGEM
DROP TABLE ITEM

SELECT * FROM PERSONAGEM
SELECT * FROM HISTORIADUNGEON
SELECT * FROM ITEM

CREATE TABLE HISTORIADUNGEON
(COD INTEGER,
 HISTORIA VARCHAR(170) NOT NULL,
 PRIMARY KEY (COD)
 );

CREATE TABLE PERSONAGEM
(NOME VARCHAR(20),
CLASSE VARCHAR(20),
VIDA INTEGER,
ULTDUN INTEGER
);

CREATE TABLE ITEM
(NOME VARCHAR(50),
DESCRICAO VARCHAR(50),
TIPO INTEGER,
POWER INTEGER
);